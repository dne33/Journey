<AnchorPane prefHeight="635.0" prefWidth="1185.0" xmlns="http://javafx.com/javafx/16"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="journey.controller.TableService">
   <VBox AnchorPane.bottomAnchor="0.0" AnchorPane.topAnchor="0.0">
      <TableView fx:id="stationTable" prefHeight="635.0" prefWidth="1190.0" VBox.vgrow="ALWAYS">
         <columns>
            <TableColumn fx:id="addressCol" prefWidth="211.0" text="Address"/>
            <TableColumn fx:id="nameCol" prefWidth="136.0" text="Name"/>
            <TableColumn fx:id="operatorCol" prefWidth="127.0" text="Operator"/>
            <TableColumn fx:id="currentTypeCol" prefWidth="52.0" text="Current"/>
            <TableColumn fx:id="connectorsCol" prefWidth="70.0" text="Connectors"/>
            <TableColumn fx:id="carparksCol" prefWidth="55.0" text="Carparks"/>
            <TableColumn fx:id="timeLimitCol" prefWidth="69.0" text="Time Limit"/>
            <TableColumn fx:id="attractionCol" prefWidth="88.0" text="Has Attraction"/>
            <TableColumn fx:id="latCol" prefWidth="95.0" text="lat"/>
            <TableColumn fx:id="longCol" prefWidth="95.0" text="long"/>
            <TableColumn fx:id="isFreeCharge" prefWidth="85.0" text="Free Charging"/>
            <TableColumn fx:id="isFreePark" prefWidth="78.0" text="Free Parking"/>
         </columns>
      </TableView>
   </VBox>
   <Button fx:id="closeButton" graphicTextGap="0.0" layoutX="1164.0" layoutY="2.0" maxHeight="20.0" maxWidth="24.0"
           minHeight="20.0" minWidth="24.0" mnemonicParsing="false" onAction="#closeScene" prefHeight="20.0"
           prefWidth="24.0" style="-fx-background-color: rgba(226,133,110)" text="X">
      <font>
         <Font size="10.0"/>
      </font>
   </Button>
</AnchorPane>
