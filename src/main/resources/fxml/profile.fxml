                        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                     </columnResizePolicy>
                   </TableView>
                  <Label layoutX="19.0" layoutY="74.0" prefHeight="19.0" prefWidth="130.0" text="Current Vehicle:">
