                        <Insets bottom="2.0" left="10.0" right="2.0" top="2.0" />
                     </padding>
                     <font>
                        <Font size="14.0" />
                     </font>
                  </Label>
                  <Button layoutX="419.0" layoutY="72.0" mnemonicParsing="false" onAction="#editCurrentVehicle" stylesheets="@../gui/buttons.css" text="Edit" />
                  <Button layoutX="472.0" layoutY="72.0" mnemonicParsing="false" onAction="#deleteCurrentVehicle" stylesheets="@../gui/buttons.css" text="Delete" />
                  <Label fx:id="warningLabel" layoutX="533.0" layoutY="85.0" textFill="#da0303">
                     <font>
                        <Font size="12.0" />
                     </font>
                  </Label>
