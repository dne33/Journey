<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="550.0" style="-fx-background-color: #C4CEA1;" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="journey.controller.ProfileController">
   <children>
      <GridPane layoutX="33.0" layoutY="33.0" prefHeight="337.0" prefWidth="483.0" style="-fx-background-color: white;">
        <columnConstraints>
          <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints maxHeight="86.0" minHeight="10.0" prefHeight="56.0" valignment="CENTER" vgrow="SOMETIMES" />
          <RowConstraints maxHeight="281.0" minHeight="10.0" prefHeight="281.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label text="MY PROFILE">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <Pane prefHeight="252.0" prefWidth="386.0" GridPane.rowIndex="1">
               <children>
                  <Label layoutX="20.0" layoutY="20.0" text="Name:">
                     <font>
                        <Font size="15.0" />
                     </font>
                  </Label>
                  <Label layoutX="21.0" layoutY="51.0" text="Vehicles:">
                     <font>
                        <Font size="15.0" />
                     </font>
                  </Label>
                  <Label fx:id="name" layoutX="80.0" layoutY="11.0" prefHeight="36.0" prefWidth="130.0" style="-fx-background-color: #E0E6B2; -fx-background-radius: 10;" text="">
                     <padding>
                        <Insets bottom="2.0" left="10.0" right="2.0" top="2.0" />
                     </padding>
                     <font>
                        <Font size="14.0" />
                     </font>
                  </Label>
                   <TableView fx:id="vehicleTable" layoutX="28.0" layoutY="81.0" prefHeight="178.0" prefWidth="432.0" VBox.vgrow="ALWAYS">
                       <columns>
                           <TableColumn fx:id="registrationCol" text="Registration" />
                           <TableColumn fx:id="makeCol" prefWidth="63.0" text="Make" />
                           <TableColumn fx:id="modelCol" prefWidth="61.0" text="Model" />
                           <TableColumn fx:id="yearCol" prefWidth="46.0" text="Year" />
                           <TableColumn fx:id="chargerTypeCol" prefWidth="103.0" text="Charger Type" />
                           <TableColumn fx:id="connectorTypeCol" prefWidth="103.0" text="Connector Type" />
                       </columns>
                     <columnResizePolicy>
                        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                     </columnResizePolicy>
                   </TableView>
                  <Label layoutX="225.0" layoutY="20.0" prefHeight="19.0" prefWidth="130.0" text="Current Vehicle:">
                     <font>
                        <Font size="15.0" />
                     </font>
                  </Label>
                  <Label fx:id="vehicle" layoutX="355.0" layoutY="12.0" prefHeight="36.0" prefWidth="113.0" style="-fx-background-color: #E0E6B2; -fx-background-radius: 10;">
                     <padding>
                        <Insets bottom="2.0" left="10.0" right="2.0" top="2.0" />
                     </padding>
                     <font>
                        <Font size="14.0" />
                     </font>
                  </Label>
               </children>
            </Pane>
         </children>
      </GridPane>
      <Button fx:id="closeButton" layoutX="519.0" layoutY="6.0" mnemonicParsing="false" onAction="#closeScene" style="-fx-background-color: white; -fx-background-radius: 10;" text="X" />
   </children>
</AnchorPane>
