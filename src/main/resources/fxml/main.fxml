<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="500.0"
            prefWidth="800.0" style="-fx-background-color: DARKGREY;" xmlns="http://javafx.com/javafx/16"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="journey.controller.MainController">
   <Pane layoutX="-7.0" layoutY="-6.0" prefHeight="59.0" prefWidth="807.0" style="-fx-background-color: white;">
      <Label alignment="CENTER" layoutX="324.0" layoutY="10.0" prefHeight="47.0" prefWidth="139.0" text="Journey"
             textAlignment="CENTER" underline="true">
         <font>
            <Font size="35.0"/>
         </font>
      </Label>
      <Button alignment="TOP_CENTER" layoutX="14.0" layoutY="19.0" mnemonicParsing="false" onAction="#userDropdown"
              prefHeight="26.0" prefWidth="104.0" text="My Profile"/>
      <Button alignment="TOP_CENTER" layoutX="689.0" layoutY="17.0" mnemonicParsing="false" onAction="#userDropdown"
              onMouseClicked="#viewMap" prefHeight="26.0" prefWidth="104.0" text="Map"/>
   </Pane>
   <Pane layoutX="21.0" layoutY="108.0" prefHeight="358.0" prefWidth="216.0" style="-fx-background-color: white;">
      <Label alignment="CENTER" layoutX="23.0" layoutY="14.0" prefHeight="26.0" prefWidth="170.0"
             text="REGISTER A VEHICLE" textAlignment="CENTER" underline="true">
         <font>
            <Font size="13.0"/>
         </font>
      </Label>
      <Label layoutX="11.0" layoutY="45.0" prefHeight="24.0" prefWidth="81.0" text="Registration:"/>
      <TextField fx:id="registrationTextBox" layoutX="100.0" layoutY="45.0" prefHeight="25.0" prefWidth="104.0"
                 promptText="Registration" style="-fx-background-color: LIGHTGREEN; -fx-border-color: BLACK;"/>
      <Label layoutX="11.0" layoutY="77.0" prefHeight="24.0" prefWidth="81.0" text="Charger Type:"/>
      <ChoiceBox fx:id="chargerBox" layoutX="100.0" layoutY="76.0" onAction="#chargerTypeChoice" prefHeight="27.0"
                 prefWidth="104.0" style="-fx-background-color: LIGHTGREEN; -fx-border-color: BLACK;"/>
      <Button layoutX="45.0" layoutY="125.0" mnemonicParsing="false" onAction="#registerVehicle"
              style="-fx-background-color: LIGHTGREEN; -fx-border-color: BLACK;" text="Register Vehicle"/>
      <Label layoutX="70.0" layoutY="166.0" text="MY STUFF">
         <font>
            <Font size="17.0"/>
         </font>
      </Label>
      <Button alignment="CENTER" contentDisplay="CENTER" layoutX="23.0" layoutY="194.0" mnemonicParsing="false"
              onAction="#viewPrevJourneys" style="-fx-background-color: LIGHTGREEN; -fx-border-color: BLACK;"
              text="View Previous Journeys"/>
   </Pane>
   <Pane layoutX="273.0" layoutY="108.0" prefHeight="358.0" prefWidth="241.0" style="-fx-background-color: white;">
      <Label alignment="CENTER" contentDisplay="CENTER" layoutX="27.0" layoutY="14.0" onMouseClicked="#viewStations"
             prefHeight="26.0" prefWidth="188.0" text="FIND A CHARGING STATION" underline="true">
         <font>
            <Font size="13.0"/>
         </font>
      </Label>
      <ComboBox fx:id="filterList" layoutX="14.0" layoutY="50.0" prefHeight="25.0" prefWidth="104.0"
                promptText="Filter" style="-fx-background-color: LIGHTGREEN; -fx-border-color: BLACK;"/>
      <ComboBox fx:id="sortList" layoutX="124.0" layoutY="50.0" prefHeight="25.0" prefWidth="104.0" promptText="Sort"
                style="-fx-background-color: LIGHTGREEN; -fx-border-color: BLACK;"/>
      <ListView fx:id="stationsList" layoutX="24.0" layoutY="93.0" prefHeight="250.0" prefWidth="200.0"
                style="-fx-border-color: BLACK;"/>
   </Pane>
   <SplitPane dividerPositions="0.4" layoutX="551.0" layoutY="108.0" orientation="VERTICAL" prefHeight="358.0"
              prefWidth="216.0">
      <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="100.0" prefWidth="160.0"
                  style="-fx-background-color: WHITE;">
         <Label alignment="CENTER" contentDisplay="CENTER" layoutX="47.0" layoutY="14.0" text="RECORD A JOURNEY"
                underline="true">
            <font>
               <Font size="13.0"/>
            </font>
         </Label>
         <Button layoutX="14.0" layoutY="43.0" mnemonicParsing="false" onAction="#startJourneyButton"
                 prefHeight="26.0" prefWidth="68.0"
                 style="-fx-background-color: LIGHTGREEN; -fx-border-color: BLACK;" text="Start"/>
         <Button layoutX="133.0" layoutY="43.0" mnemonicParsing="false" onAction="#endJourneyButton"
                 prefHeight="27.0" prefWidth="68.0"
                 style="-fx-background-color: LIGHTGREEN; -fx-border-color: BLACK;" text="End"/>
         <Label layoutX="20.0" layoutY="83.0" prefHeight="41.0" prefWidth="67.0" text="Charging Details:"
                wrapText="true"/>
         <TextField fx:id="chargingStationTextField" layoutX="97.0" layoutY="91.0" prefHeight="26.0"
                    prefWidth="104.0" style="-fx-background-color: LIGHTGREEN; -fx-border-color: BLACK;"/>
      </AnchorPane>
      <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="100.0" prefWidth="160.0"
                  style="-fx-background-color: WHITE;">
         <Label alignment="CENTER" contentDisplay="CENTER" layoutX="27.0" layoutY="14.0" prefHeight="27.0"
                prefWidth="160.0" text="STATION NOTES" textAlignment="CENTER" underline="true" wrapText="true">
            <font>
               <Font size="13.0"/>
            </font>
         </Label>
         <TextArea fx:id="stationNoteTextArea" layoutX="13.0" layoutY="43.0" prefHeight="123.0" prefWidth="188.0"
                   style="-fx-border-color: BLACK;"/>
         <Button layoutX="79.0" layoutY="176.0" mnemonicParsing="false" onAction="#submitNotes"
                 style="-fx-background-color: LIGHTGREEN; -fx-border-color: BLACK;" text="Submit"/>
      </AnchorPane>
   </SplitPane>
</AnchorPane>
