      <Button fx:id="deleteButton" layoutX="185.0" layoutY="324.0" mnemonicParsing="false" onAction="#deleteVehicle" stylesheets="@../gui/buttons.css" text="Delete Vehicle" />
   </children>
</AnchorPane>
